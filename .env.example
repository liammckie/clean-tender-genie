# Example environment variables for the Tender Genie project
# Rename this file to `.env` and fill in your values

# Public environment variables for the frontend
VITE_SUPABASE_URL="https://your-project.supabase.co"
VITE_SUPABASE_ANON_KEY="your-anon-key"

# Optional: base URL for API routes when deploying the frontend separately
VITE_API_BASE="http://localhost:3000"

# Server-side environment variables used by API routes
OPENAI_API_KEY="sk-..."
SUPABASE_URL="https://your-project.supabase.co"
SUPABASE_SERVICE_ROLE_KEY="your-service-role-key"
